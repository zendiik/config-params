{
    "name": "netleak/config-params",
    "description": "Getters to find out in which modes are the Nette application.",
    "keywords": ["nette", "config", "params"],
    "license": "MIT",
    "version": "1.3.4",
    "authors": [
		{
			"name": "Jan 'Netleak' Ženíšek",
			"email": "jan.zenisek@outlook.com"
		},
        {
            "name": "Filip Jan Pikl",
            "email": "fjpikl@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "nette/di": "~3.0"
    },
    "autoload": {
        "classmap": ["src"],
        "files": ["src/functions.php"]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "slevomat/coding-standard": "^8.13",
        "phpunit/phpunit": "^10.3",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
